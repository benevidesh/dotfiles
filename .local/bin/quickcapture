#!/bin/bash
# QN_INBOX_FILE="$HOME/Resources/journal/notes/QuickNote.txt"
DM_PROMPT="ðŸš€ QuickNote >>"
DM_OPTS='-i'

ADD_TODO=$(dmenu $DM_OPTS -p "$DM_PROMPT" < /dev/null)

